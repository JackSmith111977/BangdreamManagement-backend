<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kei.tliaswebmanagement1.mapper.EmpExprMapper">


        <!--# foreach循环遍历集合
        collection    表示要遍历的集合名称，
        item          表示集合遍历的每一个元素，
        separator     表示每遍循环的分隔符，
        open          表示循环开始前需要拼接的内容，
        close         表示循环结束后需要拼接的内容-->
    <insert id="insertBatch">
        insert into emp_exp (begin, end, company, job, emp_id) VALUES
        <foreach collection="exprList" item="expr" separator=",">
            (#{expr.begin},#{expr.end},#{expr.company},#{expr.job},#{expr.empId})
        </foreach>

    </insert>

    <!--根据员工id删除员工经历-->
    <delete id="deleteByEmpIds">
        delete from emp_exp where emp_id in
        <foreach collection="empIds" item="empId" separator="," open="(" close=")">
            #{empId}
        </foreach>
    </delete>


</mapper>